{
	"_version": "1.12.0",
	"sap.app": {
		"id": "com.co.johan.maven.openui5",
		"type": "application",
		"i18n": "i18n/i18n.properties",
		"title": "{{appTitle}}",
		"description": "{{appDescription}}",
		"applicationVersion": {
			"version": "1.0.0"
		}
	},
	"sap.ui": {
		"technology": "UI5",
		"deviceTypes": {
			"desktop": true,
			"tablet": true,
			"phone": true
		}
	},
	"sap.ui5": {
		"rootView": {
			"viewName": "com.co.johan.maven.openui5.view.App",
			"type": "XML",
			"async": true,
			"id": "Login"
		},
		"dependencies": {
			"minUI5Version": "1.60",
			"libs": {
				"sap.m": {
					
				}
			}
		},
		"models": {
			"i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"settings": {
					"bundleName": "com.co.johan.maven.openui5.i18n.i18n"
				}
			}
		},
		"routing": {
		 	"config": {
		        "routerClass": "sap.m.routing.Router",
		        "viewType": "XML",
		        "viewPath": "com.co.johan.maven.openui5.view",
		        "controlId": "App",
		        "controlAggregation": "pages",
		        "async": true
		      },
		      "routes": [
		        {
		          "pattern": "",
		          "name": "login",
		          "target": "Login"
		        },
		        {
		          "pattern": "app",
		          "name": "app",
		          "target": "App"
		        }
		      ],
		      "targets": {
		        "Login": {
		          "viewId": "login",
		          "viewName": "Login"
		        },
		        "App": {
		          "viewId": "app",
		          "viewName": "App"
		        }
		      }
		}
	}
}